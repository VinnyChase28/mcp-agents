{
  "compilerOptions": {
    "strict": true,
    "target": "ES2022",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "verbatimModuleSyntax": true,
    "declaration": true,
    "sourceMap": true,
    "composite": true,
    "declarationMap": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "useUnknownInCatchVariables": true,
    "alwaysStrict": true,
    "noUncheckedIndexedAccess": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@/apps/*": [
        "apps/*"
      ],
      "@/packages/*": [
        "packages/*"
      ],
      "@/servers/*": [
        "servers/*"
      ],
      "@mcp-agents/config": [
        "packages/config/src/index"
      ],
      "@mcp-agents/config/*": [
        "packages/config/src/*"
      ],
      "@/mcp-agents/config": [
        "packages/config/src/index"
      ],
      "@/mcp-agents/config/*": [
        "packages/config/src/*"
      ],
      "@mcp-agents/shared-types": [
        "packages/shared-types/src/index"
      ],
      "@mcp-agents/shared-types/*": [
        "packages/shared-types/src/*"
      ],
      "@/mcp-agents/shared-types": [
        "packages/shared-types/src/index"
      ],
      "@/mcp-agents/shared-types/*": [
        "packages/shared-types/src/*"
      ],
      "@mcp-agents/utils": [
        "packages/utils/src/index"
      ],
      "@mcp-agents/utils/*": [
        "packages/utils/src/*"
      ],
      "@/mcp-agents/utils": [
        "packages/utils/src/index"
      ],
      "@/mcp-agents/utils/*": [
        "packages/utils/src/*"
      ],
      "@mcp-agents/api": [
        "apps/api/src/index"
      ],
      "@mcp-agents/api/*": [
        "apps/api/src/*"
      ],
      "@/mcp-agents/api": [
        "apps/api/src/index"
      ],
      "@/mcp-agents/api/*": [
        "apps/api/src/*"
      ],
      "@mcp-agents/web": [
        "apps/web/src/index"
      ],
      "@mcp-agents/web/*": [
        "apps/web/src/*"
      ],
      "@/mcp-agents/web": [
        "apps/web/src/index"
      ],
      "@/mcp-agents/web/*": [
        "apps/web/src/*"
      ],
      "@mcp-agents/api-client-mcp": [
        "servers/api-client-mcp/src/index"
      ],
      "@mcp-agents/api-client-mcp/*": [
        "servers/api-client-mcp/src/*"
      ],
      "@/mcp-agents/api-client-mcp": [
        "servers/api-client-mcp/src/index"
      ],
      "@/mcp-agents/api-client-mcp/*": [
        "servers/api-client-mcp/src/*"
      ],
      "@mcp-agents/calculator-mcp": [
        "servers/calculator-mcp/src/index"
      ],
      "@mcp-agents/calculator-mcp/*": [
        "servers/calculator-mcp/src/*"
      ],
      "@/mcp-agents/calculator-mcp": [
        "servers/calculator-mcp/src/index"
      ],
      "@/mcp-agents/calculator-mcp/*": [
        "servers/calculator-mcp/src/*"
      ],
      "@mcp-agents/file-manager-mcp": [
        "servers/file-manager-mcp/src/index"
      ],
      "@mcp-agents/file-manager-mcp/*": [
        "servers/file-manager-mcp/src/*"
      ],
      "@/mcp-agents/file-manager-mcp": [
        "servers/file-manager-mcp/src/index"
      ],
      "@/mcp-agents/file-manager-mcp/*": [
        "servers/file-manager-mcp/src/*"
      ]
    }
  }
}
